{
  "name": "ekclick",
  "displayName": "EkClick - Vercel Deploy",
  "description": "One-click deployment to Vercel directly from VS Code",
  "version": "1.0.2",
  "publisher": "PradeepAwasthi",
  "license": "MIT",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pradeep-gif-hub/VSercel"
  },
  "bugs": {
    "url": "https://github.com/pradeep-gif-hub/VSercel/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ekclick.deployToVercel",
        "title": "EkClick Deploy to Vercel",
        "category": "EkClick"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "sharp": "^0.34.5",
    "typescript": "^5.3.3"
  }
}
