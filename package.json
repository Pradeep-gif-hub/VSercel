{
  "name": "vsercel",
  "displayName": "Vercel Deploy",
  "description": "Deploy your project to Vercel directly from VS Code",
  "version": "1.0.0",
  "publisher": "PradeepAwasthi",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/pradeep-gif-hub/VSercel"
},
"bugs": {
  "url": "https://github.com/pradeep-gif-hub/VSercel/issues"
}
,
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vsercel.deployToVercel",
        "title": "Deploy to Vercel",
        "category": "Vercel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
